[project]
	description = testing gerrit
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/jshaw86
[access "refs/for/refs/*"]
	read = group user/jshaw86
	push = group user/jshaw86
	label-Code-Review = -2..+2 group user/jshaw86
	label-Verified = -1..+1 group user/jshaw86
	rebase = group user/jshaw86
	removeReviewer = group user/jshaw86
	submit = group user/jshaw86
[access "refs/heads/*"]
	read = group user/jshaw86
	create = group user/jshaw86
	push = +force group user/jshaw86
	pushMerge = group user/jshaw86
[access "refs/tags/*"]
	push = +force group user/jshaw86
	pushTag = group user/jshaw86
	pushSignedTag = group user/jshaw86
